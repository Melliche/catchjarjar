    "autoprefixer": "^9.8.0",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.14.1",
    "postcss-cli": "^6.1.3",
    "rimraf": "^2.7.1"
  },
  "bugs": {
    "url": "https://github.com/Melliche/catchjarjar/issues"
  },
  "homepage": "https://github.com/Melliche/catchjarjar#readme",
